<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleButton">
        <attr name="circlebutton_gradientDrawable" format="reference">
            <flag name="rectangle" value="0"/>
            <flag name="oval" value="1"/>
            <flag name="line" value="2"/>
            <flag name="ring" value="3"/>
        </attr>
        <attr name="android:background"/>
        <attr name="circlebutton_background_selected" format="color"/>
        <attr name="android:textColor"/>
        <attr name="circlebutton_textColor_selected" format="color"/>
        <attr name="circlebutton_radius" format="dimension"/>
        <attr name="circlebutton_isfillet" format="boolean"/>

    </declare-styleable>
</resources>